{
  "manifest_version": 1,
  "name": "Plugin Template",
  "version": "1.0.0",
  "description": "MaiBot插件模板，提供基础的插件结构和示例代码",
  "author": {
    "name": "Your Name",
    "url": "https://github.com/yourusername"
  },
  "license": "MIT",
  "host_application": {
    "min_version": "0.8.0",
    "max_version": ""
  },
  "homepage_url": "https://github.com/MaiM-with-u/plugin-template",
  "repository_url": "https://github.com/MaiM-with-u/plugin-template",
  "keywords": ["template", "example", "starter"],
  "categories": ["utility", "template"],
  "default_locale": "zh_CN",
  "plugin_info": {
    "main_class": "TemplatePlugin",
    "dependencies": [],
    "optional_dependencies": [],
    "config_schema": {
      "enable_feature_a": {
        "type": "boolean",
        "default": true,
        "description": "启用功能A"
      },
      "sample_text": {
        "type": "string",
        "default": "Hello World",
        "description": "示例文本配置"
      }
    },
    "components": {
      "actions": ["sample_action"],
      "commands": ["sample"]
    }
  }
}